<h1>Home Page</h1>

<% posts.forEach(post => { %>
  <div class="media">
    <img src="..." class="align-self-start mr-3" alt="...">
    <div class="media-body">
      <h3 class="mt-0"><%= post.title %></h3>
      <h5><%= post.author.username %></h5>
      <%- post.body %>
    </div>
  </div>
<% }) %>